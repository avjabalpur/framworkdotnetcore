@model IEnumerable<unseentalents.models.GrantedTokens>

@{
    ViewData["Title"] = "GrantedTokens";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";

    // var webGrid = new WebGrid(source: Model);
    //var grid = new WebGrid(canPage: true, rowsPerPage: 10, ajaxUpdateContainerId: "OrderHistoryGrid", canSort: false);
    //grid.Bind(source: Model, rowCount: Model.Count, autoSortAndPage: false);
}


<div class="row event-rounds">
    <div class="col-lg-12">
        @Html.Partial("_PageTitle", "Granted tokens for users")
    </div>
</div>
<div class="panel-body">
    <div class="row">
        <div class="col-lg-12">
            @if (Model.Count() > 0)
            {
                <div class="table-responsive">
                    <table class="table table-bordered table-hover table-striped">
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.UniqueToken)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.IsActive)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.CreatedOn)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.UserName)
                                </th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.UniqueToken)
                                    </td>

                                    <td>
                                        @Html.DisplayFor(modelItem => item.IsActive)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.CreatedOn)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.UserName)
                                    </td>
                                    <td>
                                        @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                                        @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                                        @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            }
            else
            {

                <div><h4>There are no token granted!</h4></div>
            }

            <!-- /.table-responsive -->
        </div>
    </div>
    <!-- /.row -->
</div>